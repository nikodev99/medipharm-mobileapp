{
  "expo": {
    "name": "Medipharm",
    "slug": "medipharm-mobileapp",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/light/apple-icon.png",
    "scheme": "medipharmmobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/light/android-icon-72x72.png",
        "backgroundImage": "./assets/images/light/android-icon-192x192.png",
        "monochromeImage": "./assets/images/light/android-icon-96x96.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/light/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/light/apple-icon-180x180.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
